<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping>
	<class name="com.jcommerce.core.model.SessionsData"
		table="sessions_data">
		<id name="id" column="id" type="string" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="sesskey" column="sesskey" length="32" />
		<property name="expiry" column="expiry" />
		<property name="data" column="data" type="text" />
	</class>
</hibernate-mapping>