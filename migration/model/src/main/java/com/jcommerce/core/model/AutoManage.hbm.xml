<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
<hibernate-mapping>
	<class name="com.jcommerce.core.model.AutoManage"
		table="auto_manage">
		<id name="id" column="item_id" type="string" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="type" column="type" type="int" />
		<property name="startTime" column="starttime" />
		<property name="endTime" column="endtime" />
	</class>
</hibernate-mapping>